  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Debug: PRTV",
      "presentation": {
        "hidden": false,
        "group": "Debug",
        "order": 2
      },
      "runtimeExecutable": "meteor",
      "runtimeArgs": [
        "--settings=config/local.json",
        "--raw-logs"
      ],
      "env": {
        "ROOT_URL": "http://localhost:3000",
      },
      "outputCapture": "std",
      "restart": true,
      "timeout": 60000
    }
  ]
}