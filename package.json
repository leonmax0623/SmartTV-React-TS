    "start-all": "run-p start-client start-backend",
    "start-client": "run-p webpack-dev-server \"csstypes -w -s\"",
    "start-backend": "meteor run --settings config/local.json",
    "start-backend-external": "ROOT_URL=http://192.168.1.21:3007 meteor run --settings config/local.json --port 192.168.1.21:3000",
    "start-all-external": "meteor-client bundle -c client/client.config.dev.external.json -d ./client/meteor-client.js && run-p start-client-external start-backend-external",
    "webpack-dev-server-external": "webpack-dev-server --config ./webpack.client.config.js  --host 192.168.1.21",
    "start-client-external": "run-p webpack-dev-server-external \"csstypes -w -s\"",
    "webpack-dev-server": "webpack-dev-server --config ./webpack.client.config.js",
    "csstypes": "tcm -p client/**/*.pcss -o ./generated",
    "client-bundle": "meteor-client bundle -c client/client.config.dev.json -d ./client/meteor-client.js",
    "client-bundle-prod": "meteor-client bundle -c client/client.config.prod.json -d ./client/meteor-client.js",
    "build-client": "cross-env NODE_ENV=production webpack --config ./webpack.client.config.js",
    "build-backend": "meteor build ./build/server"
